class Deque:

	def __init__(self):
		self.deque = []
		self.tamanho = 0

	def vazio(self):
		if self.tamanho == 0:
			return True
		return False

	def add_frente(self, e):
		self.deque.insere(0, e)
		self.tamanho += 1

	def add_inicio(self, e):
		self.deque.insere(self.len, e)
		self.tamanho += 1

	def del_frente(self):
		if not self.empty():
			self.deque.pop(0)
			self.len -= 1

	def del_inicio(self):
		if not self.empty():
			self.deque.pop(self.len - 1)
			self.len -= 1

	def frente(self):
		if not self.empty():
			return self.deque[0]

	def inicio(self):
		if not self.empty():
			return self.deque[-1]

	def mostra(self):
		for i in self.deque:
			print(i, end=' ')
 
